exports.doc = "##################### >>>>>>>>>>>>>>  Arguements <<<<<<<<<<<<< ###################### " +
    "\n (newcol3, Chosen_Index, Chosen_Geometry, range, counter4) " + 
    "\n" +
    "\n >> Chosen_Index:" +
    "\n * 'NDWI' " +
    "\n * 'mNDWI' " +
    "\n * 'aNDWI' " +
    "\n * 'AWEIsh' " +
    "\n * 'AWEInsh' " +
    "\n * 'WI' " +
    "\n * 'TCW' " +
    "\n" +
    "\n >> Chosen_Geometry:" +
    "\n * Sample study area --> geometry " +
    "\n" +
    "\n >> newcol3:" +
    "\n * A collection of processed composites --> ImageCollection " +
    "\n" +
    "\n >> range:" +
    "\n * A range of data --> list  " +
    "\n" +
    "\n >> counter4:" +
    "\n * Size of data --> 'number' " +
    "\n" +
    "\n   ########################################################################################";

// Packages >>>

exports.init = function(newcol3, Chosen_Index, Chosen_Geometry, range, counter4) {
            
  /////////////ADDING A POINT TO MAP//////////////
  
  // Merge data
    var sensor_band_dict = ee.Dictionary({
                          l8 : ee.List([1,2,3,4,5,6,7,10]),
                          l7 : ee.List([0,1,2,3,4,6,5,9]),
                          l5 : ee.List([0,1,2,3,4,6,5,9]),
                          l4 : ee.List([0,1,2,3,4,6,5,9])  
                          });
    
    
    // Sensor band names corresponding to selected band numbers                        
    var bandNames = ee.List(['BLUE','GREEN','RED','NIR','SWIR1','SWIR2','TIR','pixel_qa']);
    
    // ------------------------------------------------------
    // Landsat 4 - Data availability Aug 22, 1982 - Dec 14, 1993
    var ls4 = ee.ImageCollection('LANDSAT/LT04/C01/T1_SR')
                .select(sensor_band_dict.get('l4'), bandNames); 
    
    // ------------------------------------------------------
    // Landsat 5 - Data availability Jan 1, 1984 - May 5, 2012 // missing 2003-01/04/2008
    var ls5 = ee.ImageCollection('LANDSAT/LT05/C01/T1_SR')
                .select(sensor_band_dict.get('l5'), bandNames); 
                
                  
    // Landsat 7 data are only used during operational SLC and
    // to fill the gap between the end of LS5 and the beginning
    // of LS8 data collection
                  
    // -------------------------------------------------------
    // Landsat 7 - Data availability Jan 1, 1999 - Aug 9, 2016
    // SLC-off after 31 May 2003
    var ls7 = ee.ImageCollection('LANDSAT/LE07/C01/T1_SR') 
                  .select(sensor_band_dict.get('l7'), bandNames);
            
             
    // --------------------------------------------------------
    // Landsat 8 - Data availability Apr 11, 2014 - present
    var ls8 = ee.ImageCollection('LANDSAT/LC08/C01/T1_SR')  
                  .select(sensor_band_dict.get('l8'), bandNames);
                
    // Merge landsat collections
    var MergedDataset = ee.ImageCollection(ls4
                  .merge(ls5)
                  .merge(ls7)
                  .merge(ls8).sort('system:time_start'));
  
  
  // **************************** select a sample for control ****************************************
  var scale = 200;
  
  // $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 0 $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$

  ////////////SELECTING IMAGES//////////////////
  
  // Load the Landsat 8 surface reflectance 
  var image_l8_0 = ee.Image(MergedDataset
      //.filterBounds(roi)
      .filterDate('2019-06-10', '2019-06-20')
      .sort('CLOUD_COVER')
      .mean());
      
  
  // Make a FeatureCollection from the hand-made geometries.
  var newfc_l8_0 = ee.FeatureCollection([
    ee.Feature(Snow_Shadow_Cloud_L8_0, {'class': 0}),
    ee.Feature(Other_L8_0, {'class': 1}),
  ]);
  /////////////TRAINING DATA//////////////

  //Extract training data from select bands of the image, print to console
  var training_l8_0 = image_l8_0.select(bandNames).sampleRegions({
     collection: newfc_l8_0,
     properties: ['class'],
     scale: scale
   });
  
  // $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 1st $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
  
  ////////////SELECTING IMAGES//////////////////
  
  // Load the Landsat 8 surface reflectance 
  var image_l8 = ee.Image(MergedDataset
      //.filterBounds(roi)
      .filterDate('2019-01-01', '2019-02-01')
      .sort('CLOUD_COVER')
      .mean());

  
  // Make a FeatureCollection from the hand-made geometries.
  var newfc_l8 = ee.FeatureCollection([
    ee.Feature(Snow_Shadow_Cloud_L8, {'class': 0}),
    ee.Feature(Other_L8, {'class': 1}),
  ]);
 
  
  /////////////TRAINING DATA//////////////
  
  //Extract training data from select bands of the image, print to console
  var training_l8 = image_l8.select(bandNames).sampleRegions({
     collection: newfc_l8,
     properties: ['class'],
     scale: scale
   });
  
  // $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 2nd $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
  
  ////////////SELECTING IMAGES//////////////////
  
  // Load the Landsat 7 surface reflectance 
  var image_l7 = ee.Image(MergedDataset
      //.filterBounds(roi)
      .filterDate('2012-01-01', '2012-02-01')
      .sort('CLOUD_COVER')
      .mean());
  
  
  // Make a FeatureCollection from the hand-made geometries.
  var newfc_l7 = ee.FeatureCollection([
    ee.Feature(Snow_Shadow_Cloud_L7, {'class': 0}),
    ee.Feature(Other_L7, {'class': 1}),
  ]);

  
  /////////////TRAINING DATA//////////////
  
  //Extract training data from select bands of the image, print to console
  var training_l7 = image_l7.select(bandNames).sampleRegions({
     collection: newfc_l7,
     properties: ['class'],
     scale: scale
   });
  
  // $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 3rd $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
  
  ////////////SELECTING IMAGES//////////////////
  
  // Load the Landsat 5 surface reflectance 
  var image_l5 = ee.Image(MergedDataset
      //.filterBounds(roi)
      .filterDate('1993-04-01', '1993-05-01')
      .sort('CLOUD_COVER')
      .mean());

  
  // Make a FeatureCollection from the hand-made geometries.
  var newfc_l5 = ee.FeatureCollection([
    ee.Feature(Snow_Shadow_Cloud_L5, {'class': 0}),
    ee.Feature(Other_L5, {'class': 1}),
  ]);

  
  /////////////TRAINING DATA//////////////
  
  //Extract training data from select bands of the image, print to console
  var training_l5 = image_l5.select(bandNames).sampleRegions({
     collection: newfc_l5,
     properties: ['class'],
     scale: scale
   });
  
  // $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$$ 4th $$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
  
  ////////////SELECTING IMAGES//////////////////
  
  // Load the Landsat 4 surface reflectance 
  var image_l4 = ee.Image(MergedDataset
      //.filterBounds(roi)
      .filterDate('1982-04-01', '1983-05-01')
      .sort('CLOUD_COVER')
      .mean());
  
  
  // Make a FeatureCollection from the hand-made geometries.
  var newfc_l4 = ee.FeatureCollection([
    ee.Feature(Snow_Shadow_Cloud_L4, {'class': 0}),
    ee.Feature(Other_L4, {'class': 1}),
  ]);


  /////////////TRAINING DATA//////////////
  
  //Extract training data from select bands of the image, print to console
  var training_l4 = image_l4.select(bandNames).sampleRegions({
     collection: newfc_l4,
     properties: ['class'],
     scale: scale
   });
   
   
   
   
    
  //>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> Merge traiang datasets <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<
  
  var training  = training_l8.merge(training_l7).merge(training_l5).merge(training_l4).merge(training_l8_0); 

  
  
  //////////////VALIDATION////////////////
  
  // Reserve some of the data for validation.
  var withRandom = training.randomColumn('random');
  var split = 0.8;  // Roughly 80% training, 20% validation.
  var training_subset = withRandom.filter(ee.Filter.lt('random', split));
  var validation_subset = withRandom.filter(ee.Filter.gte('random', split));
  
  ///////////CLASSIFYING IMAGE (80/20)//////////////
  
  // Make a Random Forest classifier and train it with 80% of data.
  var classifier = ee.Classifier.smileRandomForest(50).train({
     features: training_subset,
     classProperty: 'class',
     inputProperties: bandNames
  });
  
  var MaskedSnow = exports.Get_maskSnow(newcol3, Chosen_Index, Chosen_Geometry, range, counter4, classifier);
   
  return {
    MaskedSnow: MaskedSnow,
    Get_maskSnow: exports.Get_maskSnow,
  };
};

  
exports.Get_maskSnow = function(newcol3, Chosen_Index, Chosen_Geometry, range, counter4, classifier)
{
  function waterBodies(count2, areaList)
  {
    areaList = ee.List(areaList);
    
    var bandNames = ee.List(['BLUE','GREEN','RED','NIR','SWIR1','SWIR2','TIR','pixel_qa']);
    
    // convert to string
    var counter = ee.String(ee.Number(count2).int());
    
    // select the image
    var WATERimage0 = newcol3.filterMetadata('system:index', 'equals', ee.String(ee.Number(counter)));
                              
                              
    var WATERimage1 = ee.Image(WATERimage0.mean());
    
    // function to retrive lost metadata
    function metaData (image, imageWithMeta)
    {
      //get metadata 
      var Biva = imageWithMeta.aggregate_first('BV');
        
      var Clou = imageWithMeta.aggregate_first('cloud');
        
      var sysInd = imageWithMeta.aggregate_first('system:index');
        
      var sysTim = imageWithMeta.aggregate_first('system:time_start');
        
      //assign it
      return ee.Feature(image).set('BV', Biva).set('Cloud', Clou).set('system:index', sysInd).set('system:time_start', sysTim);
    }
      
    // obtain metadata
    var WATERimage = ee.Image(metaData(WATERimage1, WATERimage0));
    
    // Classify the Control imagery.
    var classified = WATERimage.select(bandNames).classify(classifier);
    
    var UpdateImage = WATERimage.addBands(classified.rename('SSC_Mask'));

    var FinalImage = UpdateImage.addBands(UpdateImage.select(Chosen_Index).mask(UpdateImage.select('SSC_Mask').eq(1)).unmask(-9999).rename(Chosen_Index+'_M'));
                                  

    return areaList.add(FinalImage);
    }
  
  var areaCollection = ee.ImageCollection(ee.List(counter4.iterate(waterBodies, ee.List([]))));
  
  return areaCollection;
};
